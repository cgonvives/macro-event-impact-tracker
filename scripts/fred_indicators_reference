    # ========== INFLATION DATA (Very Important) ==========
    'CPIAUCSL': {
        'name': 'Consumer Price Index (CPI)',
        'frequency': 'monthly',
        'impact': 'VERY HIGH',
        'release_schedule': 'Second week of month, 8:30 AM ET',
