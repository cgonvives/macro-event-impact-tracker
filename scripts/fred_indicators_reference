    # ========== HOUSING ==========
    'HOUST': {
        'name': 'Housing Starts',
        'frequency': 'monthly',
        'impact': 'MEDIUM',
        'release_schedule': '16th of month (unless weekend), 8:30 AM ET',
        'description': 'Number of new residential construction projects',
        'unit': 'thousands (annualized)',
