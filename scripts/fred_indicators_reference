        'source': 'Census Bureau'
    },
    
    # ========== BUSINESS SENTIMENT ==========
    'UMCSENT': {
        'name': 'University of Michigan Consumer Sentiment Index',
        'frequency': 'monthly',
        'impact': 'MEDIUM',
        'release_schedule': 'Mid-month and end-of-month',
        'description': 'Measure of consumer confidence and expectations',
        'unit': 'index',
        'market_importance': 'Predicts future consumer spending',
        'source': 'University of Michigan'
    },
    
    # ========== INTEREST RATES & SPREADS ==========
    'DFF': {
        'name': 'Effective Federal Funds Rate',
        'frequency': 'daily',
        'impact': 'VERY HIGH',
        'release_schedule': 'Daily',
        'description': 'Actual interest rate banks charge each other',
        'unit': 'percent',
        'market_importance': 'Fed policy rate - controls all other rates',
        'source': 'Federal Reserve'
    },
    'T10Y2Y': {
        'name': '10-Year / 2-Year Treasury Spread',
