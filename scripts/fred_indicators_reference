        'market_importance': 'Fed focuses on core CPI for policy decisions',
        'source': 'Bureau of Labor Statistics'
    },
    'PCEPI': {
        'name': 'Personal Consumption Expenditures (PCE) Inflation',
        'frequency': 'monthly',
        'impact': 'VERY HIGH',
        'release_schedule': 'End of month, 8:30 AM ET',
        'description': 'Broader measure of inflation including services',
        'unit': 'percent change',
        'market_importance': 'Fed\'s preferred inflation measure for policy decisions',
        'source': 'Bureau of Economic Analysis'
    },
    
    # ========== ECONOMIC ACTIVITY ==========
    'INDPRO': {
        'name': 'Industrial Production',
        'frequency': 'monthly',
        'impact': 'MEDIUM',
        'release_schedule': 'Mid-month, 9:15 AM ET',
        'description': 'Output from manufacturing, mining, utilities',
        'unit': 'index',
        'market_importance': 'Shows manufacturing health and economic activity',
        'source': 'Federal Reserve'
    },
    'RSXFS': {
        'name': 'Retail Sales (Advance Estimate)',
        'frequency': 'monthly',
        'impact': 'HIGH',
        'release_schedule': 'Mid-month, 8:30 AM ET',
        'description': 'Total sales at retail and food service stores',
        'unit': 'millions of dollars',
