def print_indicator_reference():
    """Print human-readable reference of all indicators"""
    print("\n" + "="*80)
    print("FRED INDICATORS REFERENCE")
    print("="*80)
    
    for code, info in FRED_INDICATORS_REFERENCE.items():
        print(f"\n{code}: {info['name']}")
        print(f"  Frequency: {info['frequency']}")
        print(f"  Impact: {info['impact']}")
        print(f"  Release: {info['release_schedule']}")
        print(f"  Description: {info['description']}")
        print(f"  Market Impact: {info['market_importance']}")

if __name__ == '__main__':
    print_indicator_reference()
